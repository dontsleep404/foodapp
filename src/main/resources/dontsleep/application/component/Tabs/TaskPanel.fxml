<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox alignment="CENTER_LEFT" prefHeight="6.0" prefWidth="746.0" spacing="10.0" style="-fx-background-color: #00000011; -fx-border-radius: 20;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text fx:id="txt" strokeType="OUTSIDE" strokeWidth="0.0" text="x5 Sandwich">
         <font>
            <Font name="Calibri Bold" size="19.0" />
         </font>
      </Text>
      <Pane prefHeight="36.0" prefWidth="8.0" HBox.hgrow="ALWAYS" />
      <Text fx:id="price" strokeType="OUTSIDE" strokeWidth="0.0" text="999.000">
         <font>
            <Font name="Calibri Bold" size="19.0" />
         </font>
      </Text>
      <Button fx:id="status" mnemonicParsing="false" text="Pending">
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <font>
            <Font name="Calibri Bold" size="17.0" />
         </font>
      </Button>
      <Button fx:id="cancle" cancelButton="true" mnemonicParsing="false" onAction="#cancle" style="-fx-text-fill: -color-danger-5;" text="Cancle">
         <font>
            <Font name="Calibri Bold" size="17.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
